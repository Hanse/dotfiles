set history=700
filetype plugin on
filetype indent on
set autoread
set wildmenu
set ignorecase
set smartcase
set hlsearch
set lazyredraw

syntax enable
set number
set encoding=utf8
set ffs=unix,dos,mac
set nobackup
set nowb
set noswapfile
set shiftwidth=4
set tabstop=4
set ai "Auto indent
set si "Smart indent
